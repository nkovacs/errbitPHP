{
    "name": "nkovacs/errbit-php",
    "type": "library",
    "description": "errbit/airbrake integration with php with psr-2",
    "keywords": ["errbit", "errbit php", "error tracking", "airbrake"],
    "homepage": "http://github.com/emgiezet/errbitPHP",
    "license": "MIT",
    "authors": [
        {
            "name": "flippa"
        },
        {
            "name": "emgiezet"
        },
        {
            "name": "karolsojko"
        },
        {
            "name": "deathowl"
        }
    ],
    "support": {
        "issues": "https://github.com/emgiezet/errbitPHP/issues"
    },
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpunit/phpcov": "2.0.*@dev",
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "replace": {
        "nodrew/php-airbrake": "dev-master",
        "flippa-official/errbit-php": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Errbit\\": "src/"
         }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
